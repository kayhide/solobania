{
  "name": "solobania-frontend",
  "version": "1.0.0",
  "description": "Solobania frontend",
  "private": true,
  "scripts": {
    "postinstall": "spago install",
    "clean": "rm -rf node_modules/{*,.bin,.yarn-integrity} output/* .spago/* dist/*",
    "build": "spago build && vite build",
    "dev": "vite",
    "test": "spago test",
    "format": "purty --write src && purty --write test && prettier --write src"
  },
  "keywords": [
    "purescript"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@shoelace-style/shoelace": "^2.8.0",
    "decimal.js": "^10.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "url": "^0.11.0",
    "xhr2": "^0.2.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.5",
    "postcss-import": "^15.1.0",
    "prettier": "^3.0.2",
    "pscid": "^2.10.0",
    "purescript-language-server": "^0.17.1",
    "purescript-psa": "^0.8.2",
    "purty": "^7.0.0",
    "tailwindcss": "^3.0.15",
    "vite": "^4.4.9",
    "vite-plugin-html-config": "^1.0.11"
  }
}
